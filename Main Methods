import java.util.HashMap;
import java.util.Scanner;

public class Program {
 static String a; 
 static String b;
 static String c;
 static String d;
 static Scanner scan = new Scanner(System.in);
  
 
  public static void main(String[] args) {
   Program myObj = new Program();
HashMap <Integer, Integer > playerOne = new HashMap <Integer, Integer>();
HashMap <Integer, Integer> playerTwo = new HashMap<>();

HashMap <Integer, Integer> optionOne = new HashMap<>();
optionOne.put(1, null);
optionOne.put(5, null);
optionOne.put(9, null);

HashMap <Integer, Integer> optionTwo = new HashMap<>();
optionTwo.put(2, null);
optionTwo.put(5, null);
optionTwo.put(8, null);

HashMap <Integer, Integer> optionThree = new HashMap<>();
optionThree.put(3, null);
optionThree.put(6, null);
optionThree.put(9, null);

HashMap <Integer, Integer> optionFour = new HashMap<>();
optionFour.put(1, null);
optionFour.put(2, null);
optionFour.put(3, null);

HashMap <Integer, Integer> optionFive = new HashMap<>();
optionFive.put(4, null);
optionFive.put(5, null);
optionFive.put(6, null);

HashMap <Integer, Integer> optionSix = new HashMap<>();
optionSix.put(7, null);
optionSix.put(8, null);
optionSix.put(9, null);

HashMap <Integer, Integer> optionSeven = new HashMap<>();
optionSeven.put(3, null);
optionSeven.put(5, null);
optionSeven.put(7, null);
   

System.out.println("Player 1 - Enter name.");
a = scan.nextLine();

System.out.println("Player 2 - Enter name.");
b = scan.nextLine();

System.out.println("Player 1 - Enter letter.");
c = scan.nextLine();

System.out.println("Player 2 - Enter letter.");
d = scan.nextLine();

for(int i = 0; i < 5; i++) {
  if(i == 4) {
    System.out.println(Program.a + "'s turn.");
    int moveThree = scan.nextInt();
    if(playerOne.containsKey(moveThree)) {
      System.out.println("Already chosen.");
    }
    if(playerTwo.containsKey(moveThree)) {
      System.out.println("Already chosen.");
    } else {
      playerOne.put(moveThree, null);
    }
    if(playerOne.containsKey(1) && playerOne.containsKey(5) && playerOne.containsKey(9) || playerOne.containsKey(2) && playerOne.containsKey(5) && playerOne.containsKey(8) || playerOne.containsKey(3) && playerOne.containsKey(6) && playerOne.containsKey(9) || playerOne.containsKey(1) && playerOne.containsKey(2) && playerOne.containsKey(3) || playerOne.containsKey(4) && playerOne.containsKey(5) && playerOne.containsKey(6) || playerOne.containsKey(7) && playerOne.containsKey(8) && playerOne.containsKey(9) || playerOne.containsKey(3) && playerOne.containsKey(5) && playerOne.containsKey(7)) {
  System.out.println("Congratulations. " + Program.a + " has won!");
  System.out.println("These were " + Program.a + "'s numbers.");
  for(int y : playerOne.keySet()) {
  System.out.println(y);
  
  }
  System.out.println("The game is now finished.");
  break;
  }
  if(!playerOne.equals(optionOne) || !playerOne.equals(optionTwo) || !playerOne.equals(optionThree) || !playerOne.equals(optionFour) || !playerOne.equals(optionFive) || !playerOne.equals(optionSix) || !playerOne.equals(optionSeven)) {
System.out.println("The game is finished. Nobody won.");
break;
  }
}
  System.out.println(Program.a + "'s turn.");
  int move = scan.nextInt();
  System.out.println(Program.a + " has placed an " + Program.c + " on " + move + ".");
  if(playerOne.containsKey(move)) {
    System.out.println("Already chosen.");
  }
  if(playerTwo.containsKey(move)) {
    System.out.println("Already chosen.");
  } else {
    playerOne.put(move, null);
  }
  if(playerOne.containsKey(1) && playerOne.containsKey(5) && playerOne.containsKey(9) || playerOne.containsKey(2) && playerOne.containsKey(5) && playerOne.containsKey(8) || playerOne.containsKey(3) && playerOne.containsKey(6) && playerOne.containsKey(9) || playerOne.containsKey(1) && playerOne.containsKey(2) && playerOne.containsKey(3) || playerOne.containsKey(4) && playerOne.containsKey(5) && playerOne.containsKey(6) || playerOne.containsKey(7) && playerOne.containsKey(8) && playerOne.containsKey(9) || playerOne.containsKey(3) && playerOne.containsKey(5) && playerOne.containsKey(7)) {
System.out.println("Congratulations. " + Program.a + " has won!");
System.out.println("These were " + Program.a + "'s numbers.");
for(int y : playerOne.keySet()) {
System.out.println(y);


}
System.out.println("The game is now finished.");
break;
} else {
System.out.println(Program.b + "'s turn.");
int moveTwo = scan.nextInt();
System.out.println(Program.b + " has placed an " + Program.d + " on " + moveTwo + ".");
if(playerOne.containsKey(moveTwo)) {
  System.out.println("Already chosen.");
}
if(playerTwo.containsKey(moveTwo)) {
  System.out.println("Already chosen.");
} else {
  playerTwo.put(moveTwo, null);
}
}
if(playerTwo.containsKey(1) && playerTwo.containsKey(5) && playerTwo.containsKey(9) || playerTwo.containsKey(2) && playerTwo.containsKey(5) && playerTwo.containsKey(8) || playerTwo.containsKey(3) && playerTwo.containsKey(6) && playerTwo.containsKey(9) || playerTwo.containsKey(1) && playerTwo.containsKey(2) && playerTwo.containsKey(3) || playerTwo.containsKey(4) && playerTwo.containsKey(5) && playerTwo.containsKey(6) || playerTwo.containsKey(7) && playerTwo.containsKey(8) && playerTwo.containsKey(9) || playerTwo.containsKey(3) && playerTwo.containsKey(5) && playerTwo.containsKey(7)) {
  System.out.println("Congratulations. " + Program.b + " has won!");
  System.out.println("These were " + Program.b + "'s numbers.");
  for(int k : playerTwo.keySet()) {
    System.out.println(k);
  }
  break;
}
  
}

scan.close();

}
}

  






















































